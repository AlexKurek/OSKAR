<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE oskar>

<configuration type="station" version="2.0">

    <header>
        <name value="Example Station 1"/>
        <desc value="Shows a list of antennas arranged in tiles and the use of
                 attribute sets."/>
        <date value="16/11/10"/>
        <time value="12:15"/>
    </header>

    <station>
        <longitude value="11.0"/>
        <latitude value="11.0"/>
    </station>

    <antennas generated="false">
        <antenna id="0"  x="-0.225" y="-0.225" z="0.0" import="attributeSet1"/>
        <antenna id="1"  x="-0.075" y="-0.225" z="0.0" import="attributeSet1"/>
        <antenna id="2"  x="0.075"  y="-0.225" z="0.0" import="attributeSet1"/>
        <antenna id="3"  x="0.225"  y="-0.225" z="0.0" import="attributeSet1"/>
        <antenna id="4"  x="-0.225" y="-0.075" z="0.0" import="attributeSet1"/>
        <antenna id="5"  x="-0.075" y="-0.075" z="0.0" import="attributeSet1"/>
        <antenna id="6"  x="0.075"  y="-0.075" z="0.0" import="attributeSet1"/>
        <antenna id="7"  x="0.225"  y="-0.075" z="0.0" import="attributeSet1"/>
        <antenna id="8"  x="-0.225" y="0.075"  z="0.0" import="attributeSet1"/>
        <antenna id="9"  x="-0.075" y="0.075"  z="0.0" import="attributeSet1"/>
        <antenna id="10" x="0.075"  y="0.075"  z="0.0" import="attributeSet1"/>
        <antenna id="11" x="0.225"  y="0.075"  z="0.0" import="attributeSet1"/>
        <antenna id="12" x="-0.225" y="0.225"  z="0.0" import="attributeSet1"/>
        <antenna id="13" x="-0.075" y="0.225"  z="0.0" import="attributeSet1"/>
        <antenna id="14" x="0.075"  y="0.225"  z="0.0" import="attributeSet1"/>
        <antenna id="15" x="0.225"  y="0.225"  z="0.0" import="attributeSet1"/>
    </antennas>

    <tiles>
        <tile id="0" x="0.0" y="11.1" z="0.0" antennas="0,1,2,3,4,5"/>
        <tile id="1" x="1.0" y="0.0"  z="0.0" antennas="6,7,8..15"/>
    </tiles>

    <attributeSets>
        <attributeSet1 elementPattern="fileName.dat" gain="1.0"
            gainErrorPercent="5" phase="0.0" phaseErrorPercent="10" />
    </attributeSets>

</configuration>
