<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE oskar>

<configuration type="station" version="2.0">

    <header>
        <name value="Example Station 2"/>
        <desc value="Shows antennas specfied though a generator."/>
        <date value="16/11/10"/>
        <time value="12:21"/>
    </header>

    <station>
        <longitude value="11.0"/>
        <latitude value="11.0"/>
    </station>

    <antennas generated="true">
        <number elements="1024"/>
        <pattern value="square"/>
        <separation x="1.0" y="0.5" z="0.0"/>
        <elementPattern value="filename.dat"/>
        <randomDrops percent="5"/>
        <grouping value="none"/>
    </antennas>

</configuration>
